{
  "name": "iterm2-theme-vitesse",
  "version": "0.0.1",
  "license": "MIT",
  "description": "A set of iTerm2 color themes based on the Vitesse Theme for VS Code",
  "author": "HiDeoo <github@hideoo.dev> (https://hideoo.dev)",
  "type": "module",
  "scripts": {
    "start": "tsx src",
    "lint": "prettier -c . && eslint . --cache --max-warnings=0 && tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "colord": "2.9.3",
    "degit": "2.8.4",
    "fast-glob": "3.2.12",
    "ora": "6.1.2"
  },
  "devDependencies": {
    "@hideoo/eslint-config": "0.9.0",
    "@hideoo/prettier-config": "0.1.1",
    "@hideoo/tsconfig": "0.1.0",
    "@types/degit": "2.8.3",
    "@types/node": "16.11.64",
    "eslint": "8.24.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "tsx": "3.9.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@7.13.1",
  "private": true,
  "sideEffects": false,
  "keywords": [
    "iTerm2",
    "color",
    "theme",
    "vitesse"
  ],
  "homepage": "https://github.com/HiDeoo/iterm2-theme-vitesse",
  "repository": {
    "type": "git",
    "url": "https://github.com/HiDeoo/iterm2-theme-vitesse.git"
  },
  "bugs": "https://github.com/HiDeoo/iterm2-theme-vitesse/issues",
  "prettier": "@hideoo/prettier-config",
  "lint-staged": {
    "**/*": "prettier -w -u --cache",
    "**/*.{js,jsx,ts,tsx,cjs,mjs}": "eslint --cache --max-warnings=0"
  }
}
